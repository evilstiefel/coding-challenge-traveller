{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console": false,
        "quotemark": {
            "options": ["single"]
        },
        "ordered-imports": false,
        "prefer-const": false,
        "max-line-length": {
            "options": [180]
        }
    },
    "rulesDirectory": []
}